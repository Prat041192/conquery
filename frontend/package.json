  "scripts": {
    "start": "node app/server",
    "start-production": "cross-env NODE_ENV=production PORT=8000 node app/server > webapp.log 2> webapp-err.log &",
    "lint": "eslint --ext=js lib/js test app/src/js",
    "build": "rimraf dist && gulp",
    "build-app": "cd app && rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "heroku-postbuild": "yarn run build-app",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "rimraf coverage && cross-env NODE_ENV=test nyc _mocha test",
    "test-watch": "cross-env NODE_ENV=test mocha test --watch",
    "debug-test": "cross-env NODE_ENV=test node-debug --debug-brk _mocha test",
    "prepush": "npm run lint",
    "codacy-coverage": "codacy-coverage < ./coverage/lcov.info"
  },
  "files": [
    "/dist"
  ],
