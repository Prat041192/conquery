{
  "name": "conquery",
  "version": "1.9.0-rc.1",
  "description": "conquery",
  "main": "dist/js/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0 <9.0.0",
    "yarn": ">=1.3.2"
